generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_PRISMA_URL")
}

model Employees {
  id        Int    @id @default(autoincrement())
  firstName String
  lastName  String
}

model Commands {
  commandId   String  @id @unique
  options     String
  output      String
  description String
}
